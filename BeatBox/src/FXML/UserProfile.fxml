<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox prefHeight="50.0" prefWidth="200.0">
               <children>
                  <AnchorPane prefHeight="200.0" prefWidth="200.0">
                     <children>
                        <Label prefHeight="100.0" prefWidth="200.0" text="User Name" textAlignment="CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                           <font>
                              <Font size="28.0" />
                           </font>
                        </Label>
                     </children>
                  </AnchorPane>
               </children>
            </HBox>
            <TabPane id="userProfile" fx:id="userProfile" prefHeight="450.0" prefWidth="600.0" rotateGraphic="true" tabClosingPolicy="UNAVAILABLE">
              <tabs>
                <Tab id="profileTab" fx:id="profileTab" text="">
            		<graphic>
                        <Group>
                            <Label text="Profile" />
                        </Group>
                    </graphic>
                     <content>
                        <AnchorPane prefHeight="200.0" prefWidth="200.0">
                           <children>
                              <VBox alignment="TOP_CENTER" prefHeight="500.0" prefWidth="150.0" spacing="10.0" AnchorPane.leftAnchor="50.0" AnchorPane.topAnchor="50.0">
                                 <children>
                                    <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                                       <image>
                                          <Image url="@../frameIcons/profile-icon.png" />
                                       </image>
                                    </ImageView>
                                    <Label id="profileName" fx:id="profileName" prefWidth="150.0" text="User Name">
                                       <font>
                                          <Font size="26.0" />
                                       </font>
                                    </Label>
                                    <Button id="profileNew" fx:id="profileNew" mnemonicParsing="false" text="New Profile" textFill="WHITE" />
                                    <Button id="profileSwitch" fx:id="profileSwitch" mnemonicParsing="false" text="Switch Profile" textFill="WHITE" />
                                    <Button id="profileDelete" fx:id="profileDelete" mnemonicParsing="false" text="Delete Profile" textFill="WHITE" />
                                 </children>
                              </VBox>
                           </children>
                        </AnchorPane>
                     </content>
                  </Tab>
                  <Tab id="historyTab" fx:id="historyTab" text="">
            		<graphic>
                        <Group>
                            <Label text="History" />
                        </Group>
                    </graphic>
                     <content>
                        <AnchorPane>
                           <children>
                              <TableView fx:id="history" tableMenuButtonVisible="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            						  <placeholder>
            							<Label text="No content available!" />
            						  </placeholder>
                                 <columnResizePolicy>
                                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                 </columnResizePolicy>
                              </TableView>
                           </children>
                        </AnchorPane>
                     </content>
                  </Tab>
                  <Tab id="recordsTab" fx:id="recordsTab" text="">
            		<graphic>
                        <Group>
                            <Label text="Records" />
                        </Group>
                    </graphic>
                     <content>
                        <AnchorPane prefHeight="200.0" prefWidth="200.0">
                           <children>
                              <StackPane prefHeight="150.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                 <children>
                                    <TableView fx:id="recordsTab" prefHeight="200.0" prefWidth="200.0" tableMenuButtonVisible="true" StackPane.alignment="TOP_LEFT">
                                       <placeholder>
                                          <Label text="No content available!" />
                                       </placeholder>
                                       <columns>
                                          <TableColumn fx:id="recordsCategory" prefWidth="75.0" text="Category" />
                                          <TableColumn fx:id="recordsScore" prefWidth="75.0" text="Score" />
                                       </columns>
                                       <columnResizePolicy>
                                          <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                       </columnResizePolicy>
                                    </TableView>
                                 </children>
                              </StackPane>
                           </children>
                        </AnchorPane>
                     </content>
                  </Tab>
              </tabs>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </TabPane>
         </children>
      </VBox>
   </children>
   <stylesheets>
      <URL value="@../CSS/userprofile.css" />
      <URL value="@../CSS/tableview.css" />
   </stylesheets>
</AnchorPane>
